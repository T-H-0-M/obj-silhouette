{
  "name": "obj-silhouette",
  "version": "0.1.0",
  "description": "A lightweight TypeScript utility to debug massive objects by logging their shape rather than raw content",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && prettier --write .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run typecheck && npm test && npm run build",
    "prepublishOnly": "npm run validate"
  },
  "keywords": [
    "debug",
    "shape",
    "object",
    "typescript",
    "ml",
    "tensor",
    "logging"
  ],
  "author": "Thomas Bandy",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^25.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-jest": "^28.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  },
  "dependencies": {
    "picomatch": "^4.0.3"
  }
}
