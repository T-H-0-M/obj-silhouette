{
  "name": "obj-silhouette",
  "version": "0.1.0",
  "description": "A lightweight TypeScript utility to debug massive objects by logging their shape rather than raw content",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "debug",
    "shape",
    "object",
    "typescript",
    "ml",
    "tensor",
    "logging"
  ],
  "author": "Thomas Bandy",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^25.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "^5.9.3"
  }
}
